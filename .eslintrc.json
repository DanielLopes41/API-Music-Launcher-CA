{
  "extends": ["@rocketseat/eslint-config/node"],
  "rules": {
    "semi": ["off"],
    "no-extra-semi": "off",
    "no-extra-parens": "off",
    "no-unexpected-multiline": "off",
    "arrow-parens": ["off"],
    "function-paren-newline": ["off"],
    "wrap-iife": ["off"],
    "indent": ["error", 2],
    "no-useless-constructor": "off"
  }
}
